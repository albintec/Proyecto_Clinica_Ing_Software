         <div class="container mt-4">
            <div class="card border-secondary">
            <div class="card-header">Crear Registro</div>
            <div class="card-body">
             <form action="/create" method="POST">
                 <div class="mb3">
                     <label for="nombre" class="form-label">Nombre</label>
                     <input type="text" class="form-control" id="nombre" name="nombre" tabindex="1">
                 </div>
                 <div class="mb3">
                    <label for="apellido" class="form-label">Apellido</label>
                    <input type="text" class="form-control" id="apellido" name="apellido" tabindex="1">
                </div>
                <div class="mb3">
                    <label for="especialidad" class="form-label">Especialidad</label>
                    <select name="especialidad" id="especialidad" class="form-select" aria-label="Default select example">
                           <option value="Odontología" selected>Odontología</option>
                           <option value="Oftalmología" selected>Oftalmología</option>
                           <option value="Medicina General" selected>Medicina General</option>
                           <option value="Cardiología" selected>Cardiología</option>
                        </select>
                </div>
                <div class="mb3">
                    <label for="telefono" class="form-label">Telefono</label>
                    <input type="text" class="form-control" id="telefono" name="telefono" tabindex="1">
                </div>
                <br>
                <button type="submit" class="btn btn-primary" tabindex="5">Guardar</button>
                <a href="admin" class="btn btn-secondary">Cancelar</a>
             </form>
           </div>
           </div>
        </div> 
       <br> 






    <!--FOOTER-->
