        </ul>
        <span class="btn_menu">
            <i class="fa fa-bars"></i>
        </span>
    </nav>
     <h1>Bienvenido al panel de administrador</h1>
     
     <section class="tablas">
     <div class="container">
         <a href="create" class="btn btn-outline-primary mt-4"><i class='bx bxs-plus-circle bx-md' ></i></a>
         <table class="table table-bordered table-striped text-center mt-4">
             <thead>
                 <tr class="bg-primary text-white">
                 <th scope="col">ID</th>
                 <th scope="col">Nombre</th>
                 <th scope="col">Apellido</th>
                 <th scope="col">Especialidad</th>
                 <th scope="col">Telefono</th>
                 <th scope="col">Acciones</th>
                </tr>                 
             </thead>
             <tbody>
                 <% results.forEach((doctores)=> {%>
                 <tr>
                     <td><%= doctores.id_doctor %></td>
                     <td><%= doctores.nombre%></td>
                     <td><%= doctores.apellido%></td>
                     <td><%= doctores.especialidad%></td>
                     <td><%= doctores.telefono%></td>
                     <td>
                         <a href="/edit/<%= doctores.id_doctor %>" class="btn btn-outline-info">Editar</a>
                         <a href="/admin/<%= doctores.id_doctor %>" class="btn btn-outline-danger">Eliminar</a>
                     </td>
                 </tr>
                 <%}) %>
             </tbody>
         </table>

    </div>
</section>
